Options -MultiViews
RewriteEngine On
RewriteBase /

RewriteCond %{HTTPS} !=on
RewriteCond %{HTTP_HOST} ^pataspare\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.pataspare\.com$ [OR]
RewriteCond %{HTTP_HOST} ^pataspare\.co\.ke$ [OR]
RewriteCond %{HTTP_HOST} ^www\.pataspare\.co\.ke$
RewriteRule .* https://pataspare.com%{REQUEST_URI} [R=301,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^ index.html [QSA,L]
